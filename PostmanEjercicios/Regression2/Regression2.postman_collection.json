				{
					"name": "Verify pet by status",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"findMyPet = function(pm){\r",
									"    let jsonData = pm.response.json();\r",
									"    for( let i = 0; i < jsonData.length; i++){\r",
									"        if (jsonData[i].id === parseInt(pm.collectionVariables.get(\"id\"))){\r",
									"           return jsonData[i].status;\r",
									"        }\r",
									"    }\r",
									"}"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"status200oK(pm);\r",
									"\r",
									"pm.test(\"Pet is in the right list\", function(){\r",
									"    pm.expect(findMyPet(pm)).to.eql(pm.collectionVariables.get(\"status\"))\r",
									"})\r",
									"\r",
									""
								],
								"type": "text/javascript"
							}
						}
					],
