									"incluyePoke = function(pm,pokeName){\r",
									"        let respuesta = pm.response.json();\r",
									"        let listaPoke = respuesta.results;\r",
									"        let incluye = false;\r",
									"\r",
									"        listaPoke.forEach((pokemon)=>{\r",
									"            if(pokemon.name === pokeName){\r",
									"                incluye = true;\r",
									"            }\r",
									"        })\r",
									"        \r",
									"    pm.test(\"incluye el Pokemon\", function() {\r",
									"        pm.expect(incluye).to.eql(true);\r",
									"    })\r",
									"}"
								],
								"type": "text/javascript"
							}
						},
